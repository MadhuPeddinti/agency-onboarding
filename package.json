{
  "name": "agency-onboarding-api",
  "version": "1.0.0",
  "description": "Agency Onboarding Multi-step Form API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev1": "docker compose -f docker-compose.dev.yml --env-file .env up --build",
    "docker:dev:detached": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f app",
    "docker:dev:restart": "docker-compose -f docker-compose.dev.yml restart && docker-compose -f docker-compose.dev.yml logs -f app",
    "docker:dev:clean": "docker-compose -f docker-compose.dev.yml down -v",
    "local:dev": "nodemon server.js",
    "local:debug": "nodemon --inspect=9229 server.js"
  },
  "nodemonConfig": {
    "watch": [
      "*.js",
      "**/*.js"
    ],
    "ignore": [
      "node_modules",
      "uploads"
    ],
    "ext": "js,json",
    "env": {
      "NODE_ENV": "development"
    },
    "delay": "1000"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
